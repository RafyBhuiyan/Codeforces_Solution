#include<bits//stdc++.h>
#include<string>
using namespace std;
int main()
{
    string ra;
    cin>> ra;
    int l = ra.length();
    for(int i = 0; i<l;i++){
        if(ra[i]=='W' && ra[i+1]=='U' && ra[i+2]=='B'){
            if(i>0)
                printf(" ");
            i+=2;
        }
        else
            printf("%c",ra[i]);
    }
    return 0;
}
