#include<bits/stdc++.h>
#include<string.h>
#define ll long long int
#define br <<"\n"
#define For(i,n) for(int i=0;i<n;i++)
#define Forran(i,n,a) for(int i=n;i<a;i++)
#define Forrev(i,n) for(int i = n-1; i >= 0; i--)
#define fastio ios_base::sync_with_stdio(0); cin.tie(0); cout.tie(0);
using namespace std;
int main()
{
    fastio
    int t,x,y,a=0;
    char b;
    cin>>x>>y;
    t=x*y;
    while(t--)
    {
        cin>>b;
        if(b=='W'||b=='B'||b=='G');
        else{
            a++;
        }
    }
    if(a){
        cout<<"#Color";
    }
    else
    {
        cout<<"#Black&White";
    }
    return 0;
}
