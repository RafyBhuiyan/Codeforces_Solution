#include<bits/stdc++.h>
#include<string.h>
using namespace std;
int main()
{
    int x,sum=0;
    string ra;
    cin>>x;
    while(x--)
    {
        cin>>ra;
        if(ra[0]=='T') sum+=4;
        else if(ra[0]=='C') sum+=6;
        else if(ra[0]=='O') sum+=8;
        else if(ra[0]=='D') sum+=12;
        else if(ra[0]=='I') sum+=20;
    }
    cout<<sum;
    return 0;
}
