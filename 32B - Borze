#include<bits/stdc++.h>
using namespace std;
int main()
{
    int l;
    string ra;
    cin>>ra;
    l=ra.length();
    for(int i=0;i<l;i++){
        if(ra[i]=='.')
            cout<<"0";
        else
            if(ra[++i]=='.')
                cout<<"1";
            else
                cout<<"2";
    }
    return 0;
}
