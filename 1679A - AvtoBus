#include<bits/stdc++.h>
#define ll long long int
#define fastio ios_base::sync_with_stdio(0); cin.tie(0); cout.tie(0);
#define for(i,n) for(int i=0;i<n;i++)
#define br <<"\n";
using namespace std;
int main()
{
    fastio
    ll t,n;
    cin>>t;
    while(t--)
    {
        cin>>n;
        if(n<4 || n%2!=0)
        {
            cout<<"-1" br;
        }
        else
        {
            if(n%6==0)
                cout<<n/6<<" ";
            else
                cout<<n/6 +1<<" ";
            cout<< n/4 br;
        }
    }
    return 0;
}
