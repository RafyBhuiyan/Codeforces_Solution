#include<bits/stdc++.h>
#define ll long long int
#define fastio ios_base::sync_with_stdio(0); cin.tie(0); cout.tie(0);
#define br <<"\n";
#define for(i,n) for(i=0;i<n;i++)
using namespace std;
int main()
{
    int t,x,odd,i,a;
    cin>>t;
    while(t--)
    {
        cin>>x;
        odd=0;
        for(i,x)
        {
            cin>>a;
            if(a%2!=0) odd++;
        }
        if(odd%2==0){
            cout<<"YES" br;
        }
        else{
            cout<<"NO" br;}
    }
    return 0;
}
