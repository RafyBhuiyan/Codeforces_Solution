#include<bits/stdc++.h>
using namespace std;
int main()
{
    int t,i,j,m=0,c=0;
    cin>>t;
    while(t--)
    {
        cin>>i>>j;
        if(i>j)
            m++;
        else if(i<j)
            c++;
    }
    if(m>c)
        cout<<"Mishka";
    else if(m<c)
        cout<<"Chris";
    else
        cout<<"Friendship is magic!^^";
    return 0;
}
