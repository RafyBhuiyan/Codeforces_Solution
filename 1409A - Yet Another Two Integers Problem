#include<bits/stdc++.h>
using namespace std;
int main()
{
    int a,b,t,i=0,x;
    cin>>t;
    while(t--)
    {
        cin>>a>>b;
        if(a>=b)
        {
            x=(a-b)/10;
            ((a-b)-x*10>0)?i=1:i=0;
            cout<< x +i <<endl;
        }
        else
        {
            x=(b-a)/10;
            ((b-a)-x*10>0)?i=1:i=0;
            cout<<x+i<<endl;
        }
    }
    return 0;
}
