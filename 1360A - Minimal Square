#include<bits/stdc++.h>
#include<string.h>
#define ll long long int
#define br <<"\n";
#define For(i,n) for(int i=0;i<n;i++)
#define Forran(i,n,a) for(int i=n;i<a;i++)
#define Forrev(i,n) for(int i = n-1; i >= 0; i--)
#define fastio ios_base::sync_with_stdio(0); cin.tie(0); cout.tie(0);
using namespace std;
int main()
{
    fastio
    int t,a,b,x,y;
    cin>>t;
    while(t--)
    {
        cin>>a>>b;
        x=min(a,b);
        y=max(a,b);
        x*=2;
        if(x<y)
            x=y;
        cout<<x*x br;
    }
    return 0;
}
