#include<bits/stdc++.h>
#include<string.h>
#define ll long long int
#define br <<"\n";
#define FOR(i,n) for(int i=0;i<n;i++)
#define FORran(i,n,a) for(int i=n;i<a;i++)
#define FORrev(i,n) for(int i = n-1; i >= 0; i--)
#define fastio ios_base::sync_with_stdio(0); cin.tie(0); cout.tie(0);
using namespace std;
int main()
{
    fastio
    int a,b,maxx;
    cin>>a>>b;
    maxx=max(a,b);
    maxx=7-maxx;
    if(maxx==6){
        cout<<"1/1";
    }
    else if(maxx%2==0){
        cout<<maxx/2<<"/3";
    }
    else if(maxx==3){
        cout<<"1/2";
    }
    else{
        cout<<maxx<<"/6";
    }
    return 0;
}
