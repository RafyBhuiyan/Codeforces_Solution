#include<bits/stdc++.h>
#include<string.h>
#define ll long long int
#define br <<"\n";
#define For(i,n) for(int i=0;i<n;i++)
#define Forran(i,n,a) for(int i=n;i<a;i++)
#define Forrev(i,n) for(int i = n-1; i >= 0; i--)
#define fastio ios_base::sync_with_stdio(0); cin.tie(0); cout.tie(0);
using namespace std;
int main()
{
    fastio
    int t,a,x,y;
    cin>>t;
    while(t--)
    {
        cin>>a;
        y=a%10;
        if(a>1000)
            x=10;
        else if(a>100)
            x=6;
        else if(a>10)
            x=3;
        else
            x=1;
        cout<<(y-1)*10 + x br;
    }
    return 0;
}
