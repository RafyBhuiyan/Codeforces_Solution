#include<bits/stdc++.h>
#include<string.h>
#define ll long long int
#define br <<"\n";
#define For(i,n) for(int i=0;i<n;i++)
#define Forran(i,n,a) for(int i=n;i<a;i++)
#define Forrev(i,n) for(int i = n-1; i >= 0; i--)
#define fastio ios_base::sync_with_stdio(0); cin.tie(0); cout.tie(0);
using namespace std;
int main()
{
    fastio
    char a;
    string ra="codeforces";
    int l = ra.length(),temp=0,t;
    cin>>t;
    while(t--){
        temp=0;
        cin>>a;
        For(i,l){
            if(a==ra[i]){
                temp=1;
                break;
            }
        }
        if(temp){
            cout<<"YES" br;
        }
        else
            cout<<"NO" br;
    }
    return 0;
}
