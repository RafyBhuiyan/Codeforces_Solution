#include<bits/stdc++.h>
using namespace std;
int main()
{
    int l,t;
    cin>>t;
    while(t--)
    {
        string ra;
        cin>>ra;
        l=ra.length();
        for(int i=0; i<l; i++)
        {
            if(i==0||i%2!=0)
               cout<<ra[i];
        }
        cout<<endl;
    }
    return 0;
}
